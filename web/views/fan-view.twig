<script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.2/jquery.min.js"></script>
<script src = "{{ app.request.getBaseURL() ~ '/assets/js/app.js' }}" ></script>
<script src = "https://connect.facebook.net/en_US/all.js" ></script>

<head>
    <title>php-sdk</title>
    <style>
      body {
        font-family: 'Lucida Grande', Verdana, Arial, sans-serif;
      }
      h1 a {
        text-decoration: none;
        color: #3b5998;
      }
      h1 a:hover {
        text-decoration: underline;
      }
    </style>
    
    <a href="https://www.facebook.com/dialog/pagetab?app_id={{ pageUrl }}" >Add page tabs</a>
    
    <a class="button button_blue" id="config_refresh_pages" onclick="loadFanPages();">Show fanpages</a>
    {% if active == "image" %}
    <img src="{{ image }}">
    {% endif %}
    <!--<div>{{ html }}</div>-->
    {% if active == "iframe" %}
         <iframe width="810" height={{ iframe_height}} scrolling="true" frameborder="0" scrolling="no" marginheight="0" 
          marginwidth="0" src="{{ iframe }}">
        </iframe>
    {% endif %}
    
  </body>